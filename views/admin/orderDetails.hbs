        <!-- Navbar Start -->
        <nav class="navbar navbar-expand bg-light navbar-light sticky-top px-4 py-0">
            <a href="index.html" class="navbar-brand d-flex d-lg-none me-4">
                <h2 class="text-primary mb-0"><i class="fa fa-hashtag"></i></h2>
            </a>
             <a href="/admin/adminDash" class="navbar-brand mx-4 mb-3">
                    <h3 class="text-primary"><img  class=""
                  src="data:image/jpeg;base64,"
                  alt=""></h3>
                </a>          
            <div class="navbar-nav align-items-center ms-auto">


                <div class="nav-item dropdown">
                    <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown">
                        <span class="d-none d-lg-inline-flex">{{admin.name}}</span>
                    </a>
                    <div class="dropdown-menu dropdown-menu-end bg-light border-0 rounded-0 rounded-bottom m-0">
                        <a href="#" class="dropdown-item">My Profile</a>

                        <a href="/admin/adminLogout" class="dropdown-item">Log Out</a>
                    </div>
                </div>
            </div>
        </nav>
        <!-- Navbar End -->

        <section class="h-100 gradient-custom pb-5">
            <div class="container  ">
                <div class=" pb-5">
                    <div class="row d-flex justify-content-center align-items-center h-100">
                        <div class="col-lg-10 col-xl-8">
                            <div class="card" style="border-radius: 10px;">

                                <div class="card-body p-4">
                                    <div class="d-flex justify-content-between align-items-center mb-4">
                                        <p class="lead fw-normal mb-0" style="color: #1c1b19;">Receipt</p>
                                        <p class="small text-muted mb-0">Receipt Voucher : {{order._id}}</p>
                                    </div>

                                    {{#each order.product}}
                                    <div class="card shadow-0 border d-flex justify-content-between align-items-center">
                                        <div class="card-body">
                                            <div class="row">
                                                <div class="col">
                                                    <img src="/imgUploads/{{pro_Id.image.[0].image1}}" class="img-fluid"
                                                        alt="img">
                                                </div>
                                                <div class="col text-center ">
                                                    <p class="text-muted mb-0">{{pro_Id.productName}}</p>
                                                </div>
                                                <div class="col text-center ">
                                                    <p class="text-muted mb-0 small">x {{this.quantity}}</p>
                                                </div>

                                                <div class="col text-center ">
                                                    <p class="text-muted mb-0 small">₹{{this.subTotal}}</p>
                                                </div>
                                                <div class="col text-center ">
                                                    <p class="text-muted mb-0 small">status : <span
                                                            style="color:orangered">
                                                            {{../order.status}}</span></p>
                                                </div>

                                                <div class="col text-center ">
                                                    <form method="post" action="/admin/changeOrderStatus"
                                                        id="statusForm">

                                                        <select class="form-control" id="exampleFormControlSelect1"
                                                            name="orderStatus">
                                                            <option>Status</option>
                                                            <option>Ordered</option>
                                                            <option>Shipped</option>
                                                            <option>Delivered</option>
                                                            <option>Cancelled</option>

                                                        </select>
                                                        <input type="text" name="orderId" value="{{../order._id}}"
                                                            hidden>
                                                        <input type="text" name="proId" value="{{this._id}}" hidden>
                                                        <a><button type="submit" class="btn"
                                                                style="background-color:rgb(231, 219, 219)"
                                                                href="#">Change</button></a>

                                                    </form>
                                                </div>


                                            </div>
                                            <div class="row d-flex align-items-center">

                                            </div>
                                        </div>
                                    </div>

                                    {{/each}}


                                    <div class="d-flex justify-content-between pt-2">
                                        <p class="fw-bold mb-0"><span style="color:#1b1a19;">Order Details</span>
                                        </p>
                                        <p class="text-muted mb-0"><span class="fw-bold me-4">Subtotal : </span> ₹
                                            {{order.Total}}</p>
                                    </div>



                                    <div class="d-flex justify-content-between">
                                        <p class="text-muted mb-0"><span class="fw-bold me-4">Invoice Date :</span>
                                            {{order.ordered_on}}</p>

                                    </div>

                                    <div class="d-flex justify-content-between ">
                                        {{!-- <p class="text-muted mb-0">Recepits Voucher : 18KU-62IIK</p> --}}
                                        <p class="text-muted mb-0"><span class="fw-bold me-4">Shipping Charge :
                                            </span>
                                            ₹
                                            {{order.ShippingCharge}}</p>
                                    </div>
                                    <div class="d-flex justify-content-between ">
                                        {{!-- <p class="text-muted mb-0">Recepits Voucher : 18KU-62IIK</p> --}}
                                        <p class="text-muted mb-0"><span class="fw-bold me-4">Coupon Discount :
                                            </span>
                                            ₹
                                            {{order.couponDiscount}}</p>
                                    </div>
                                    <div class="d-flex justify-content-between ">
                                        {{!-- <p class="text-muted mb-0">Recepits Voucher : 18KU-62IIK</p> --}}
                                        <p class="text-muted mb-0"><span class="fw-bold me-4">Grand Total : </span>
                                            ₹
                                            {{order.grandTotal}}</p>
                                    </div>

                                </div>
                                <div class="card-footer border-0 px-4 py-5 "
                                    style="background-color: #131211; border-bottom-left-radius: 10px; border-bottom-right-radius: 10px;">
                                    <h5
                                        class="d-flex align-items-center justify-content-end text-white text-uppercase mb-0">
                                        Total
                                        paid: <span class="h2 mb-0 ms-2">₹{{order.grandTotal}}</span></h5>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>


        <!-- Footer Start -->
        <div class="container-fluid pt-4 px-4">
            <div class="bg-light rounded-top p-4">
                <div class="row">
                    <div class="col-12 col-sm-6 text-center text-sm-start">
