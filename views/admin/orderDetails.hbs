    <script>
        $("#statusForm").submit((e) => {
            e.preventDefault()
            $.ajax({
                url: '/admin/changeOrderStatus',
                method: 'post',
                data: $('#statusForm').serialize(),
                success: (response) => {
            alert('dsfsafsdf')
                    if (response.modified) {
                        location.reload()
                    } else {
                        alert('not changed')
                    }
                }
            })
        })
    </script>


